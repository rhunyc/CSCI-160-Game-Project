public class Enemy {
    private String name;
    private int hP;
    private int damage;
    
    private Enemy(String name, int hP, int damage){
        this.name = name;
        this.hP = hP;
        this.damage = damage;
    }
    
    
    public Enemy spawnHeadCrab(){
        Enemy headCrab = new Enemy("Headcrab", 5, 2);
        return headCrab;
    }
    
    public Enemy spawnSoldier(){
        Enemy soldier = new Enemy("Soldier", 10, 5);
        return soldier;
    }
    
